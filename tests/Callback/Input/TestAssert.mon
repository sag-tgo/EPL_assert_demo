using com.apama.exceptions.Exception;
using com.apamax.test.callback.Assert;
using com.apamax.test.AssertHelper;

monitor TestAssert {

	action logAtError(string message) {
		log message at ERROR;
	}

	action onload() {
		Assert asserter := Assert.create(AssertHelper.#die);
		asserter.#true(false, "the sky is blue");
		asserter.equal(2 + 2, 5, "Two plus two = four");
	}
}
